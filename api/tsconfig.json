{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "typeRoots": ["../node_modules/@types"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}